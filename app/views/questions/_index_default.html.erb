<section class="row">
  <% unless @question_count.zero? %>
    <% @questions.each do |question| %>
      <%=render 'index_media',question: question %>
    <% end %>
    <div class="col-12">
      <%= paginate @questions %>
    </div>
  <% else %>
    <div class="question col-12">
      <%= t(:no_data) %>
    </div>
  <% end %>
  <div class="col-12">
    <%=link_to t(:new_question),new_question_path,class: 'btn btn-primary btn-large' %>
  </div>
</section>